{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Host": {
    "Id": "#{HOST_ID}#",
    "Secret": "#{HOST_SECRET}#"
  },
  "Api": {
    "BaseUrl": "https://localhost:7001",
    "TokenEndpoint": "https://localhost:7001/api/auth/token"
  },
  "HostConfiguration": {
    "ServerUrl": "https://localhost:7001",
    "MaxConcurrentSessions": 5,
    "EnableRecording": true,
    "EnableFileTransfer": true,
    "EnableClipboard": true,
    "EnableAudio": false
  },
  "RemoteControlProvider": {
    "Type": "ControlR",
    "Settings": {
      "LicenseKey": "#{CONTROLR_LICENSE}#",
      "ServerUrl": "#{CONTROLR_SERVER}#"
    }
  }
}