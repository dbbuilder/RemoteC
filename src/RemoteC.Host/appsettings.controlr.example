{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Host": {
    "Id": "#{HOST_ID}#",
    "Secret": "#{HOST_SECRET}#"
  },
  "Api": {
    "BaseUrl": "https://api.remotec.example.com",
    "TokenEndpoint": "/api/auth/host/token"
  },
  "HostConfiguration": {
    "ServerUrl": "https://api.remotec.example.com",
    "MaxConcurrentSessions": 5,
    "EnableRecording": true,
    "EnableFileTransfer": true,
    "EnableClipboard": true,
    "EnableAudio": false
  },
  "RemoteControlProvider": {
    "Type": "ControlR",
    "Settings": {
      // ControlR provider requires license key and server URL
      "LicenseKey": "#{CONTROLR_LICENSE}#",
      "ServerUrl": "#{CONTROLR_SERVER}#"
    }
  }
}